{
  "name": "praetorius",
  "version": "0.1.57",
  "type": "module",
  "license": "MIT",
  "bin": {
    "praetorius": "src/cli/index.js",
    "prae": "src/cli/index.js"
  },
  "files": [
    "dist",
    "src/cli",
    "ui",
    "scaffolds",
    "assets"
  ],
  "engines": {
    "node": ">=18.17"
  },
  "exports": {
    ".": {
      "import": "./dist/praetorius.es.js",
      "require": "./dist/praetorius.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "node -e \"process.exit(0)\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run -t unit",
    "test:e2e": "vitest run -t e2e",
    "e2e:smoke": "node scripts/e2e-smoke.mjs",
    "prepack": "node scripts/prepack-verify.mjs && chmod +x src/cli/index.js",
    "prepublishOnly": "npm run test && npm pack --dry-run",
    "changelog": "conventional-changelog -p conventionalcommits -i CHANGELOG.md -s",
    "prepare": "husky"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@vitest/coverage-v8": "^4.0.8",
    "conventional-changelog-cli": "^4.1.0",
    "execa": "^9.6.0",
    "husky": "^9.1.7",
    "vite": "^7.2.2",
    "vitest": "^4.0.8"
  },
  "dependencies": {
    "ajv": "^8.17.1",
    "commander": "^12.1.0",
    "csv-parse": "^5.5.3",
    "enquirer": "^2.4.1",
    "esbuild": "^0.26.0",
    "picocolors": "^1.0.0",
    "update-notifier": "^7.3.1",
    "yaml": "^2.5.0"
  },
  "keywords": [
    "cli",
    "portfolio",
    "generator",
    "scaffold",
    "praetorius"
  ],
  "homepage": "https://github.com/cbassuarez/praetorius#readme",
  "bugs": {
    "url": "https://github.com/cbassuarez/praetorius/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cbassuarez/praetorius.git"
  }
}
