{
  "name": "prae",
  "version": "0.1.0",
  "type": "module",
  "bin": {
    "prae": "bin/prae.cjs"
  },
  "license": "MIT",
  "files": ["dist", "src/cli", "bin", "scaffolds"],
  "bin": {
    "prae": "src/cli/index.js"
  },
  "exports": {
    ".": {
      "import": "./dist/praetorius.es.js",
      "require": "./dist/praetorius.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "prae": "node ./bin/prae.cjs",
    "prae:init": "node ./bin/prae.cjs init",
    "prae:validate": "node ./bin/prae.cjs validate",
    "prae:build": "node ./bin/prae.cjs build",
    "prae:build:single": "node ./bin/prae.cjs build --single",
    "prae:preview": "node ./bin/prae.cjs preview"
  },
  "devDependencies": { "vite": "^5.4.0" },
  "dependencies": {
    "ajv": "^8.17.1",
    "commander": "^12.1.0",
    "enquirer": "^2.4.1",
    "express": "^4.19.2",
    "picocolors": "^1.0.0"
  },
  "engines": { "node": ">=18" },
  "publishConfig": { "access": "public" },
  "keywords": ["cli", "portfolio", "generator", "praetorius", "seb-suarez"]
}
